<p id="notice"><%= notice %></p>

<h1>Bill Tos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bill_tos.each do |bill_to| %>
      <tr>
        <td><%= bill_to.name %></td>
        <td><%= link_to 'Show', bill_to %></td>
        <td><%= link_to 'Edit', edit_bill_to_path(bill_to) %></td>
        <td><%= link_to 'Destroy', bill_to, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bill To', new_bill_to_path %>
